!Xft.rgba: none
!Xft.hintstyle: hintfull
URxvt.buffered: true
URxvt.background: #161b21
URxvt.foreground: white
URxvt.cursorColor: white
URxvt.underlineColor: #5232b6
URxvt.font: xft:Hack:size=11, \
            xft:TerminessTTFNerdFontMono:size=11
!URxvt.perl-ext-common:  default,tabbed
urxvt*scrollBar: false
!Urxvt.letterSpace: -1
!------------
!True Transparency
!------------
!set to 32-bit for real transparency (compositing required (see xcompmgr)
urxvt*depth:             32
!transparent=0000 opaque=ffff
urxvt*background: rgba:1111/1111/1111/dddd
!urxvt*background: rgba:1111/1111/1111/0000


!------------------
!False tranparency
!------------------
!URxvt.transparent: true
!URxvt.shading: 50 

Urxvt.secondaryScroll:  true    # Enable Shift-PageUp/Down in screen

!Leather color scheme
 
*.foreground:   #e6d4c3
*.background:   #282222
*.cursorColor:  #e6d4c3
*.color0:       #222222
*.color8:       #331c1c
*.color1:       #997373
*.color9:       #995454
*.color2:       #999973
*.color10:      #999954
*.color3:       #997e63
*.color11:      #cc9e70
*.color4:       #738099
*.color12:      #7090cc
*.color5:       #857399
*.color13:      #a685cc
*.color6:       #7a9998
